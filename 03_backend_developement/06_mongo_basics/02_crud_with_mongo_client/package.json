{
  "name": "crud-with-client",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "mongo mongo-basics -u mongo-basics-app -p password < utils/resetDatabase.js && ts-node ./src/index.ts",
    "test": "eslint --ext .js,.ts . && jest"
  },
  "devDependencies": {
    "@types/jest": "26.0.9",
    "@types/mongodb": "3.5.25",
    "@types/node": "14.0.27",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "camp2-helpers": "fewlines-education/camp2-helpers#main",
    "eslint": "7.6.0",
    "jest": "26.3.0",
    "mongodb": "3.6.0",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "ignorePatterns": [
      "jest.config.js",
      "node_modules/",
      "*.test.ts",
      "src.sample/*"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 1
    }
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testEnvironment": "node"
  }
}